# ログレベル(debug, info, warn, error)
LOG_LEVEL=info
# ログフォーマット(local, cloud)
# local: 開発時向けの見やすさ重視の簡易なログ
# cloud: CloudWatch等で解析する前提の構造化ログ
LOG_FORMAT=local

# API Server
API_PORT=8080
API_KEY=dummy
# Graceful shutdown timeout in seconds (default: 5)
SHUTDOWN_TIMEOUT=5

# Database
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=api_db
DB_SSLMODE=disable
